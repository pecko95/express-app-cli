{
  "name": "express-app-cli",
  "version": "0.0.1",
  "description": "CLI app for generating an express project folder structure",
  "main": "build/app.js",
  "bin": "build/app.js",
  "scripts": {
    "start": "tsc build && node src/app.js",
    "start-ts": "ts-node src/app.ts",
    "watch": "nodemon src/app.ts",
    "build": "tsc --outDir ./build"
  },
  "keywords": [
    "CLI",
    "app",
    "nodejs",
    "express",
    "javascript",
    "typescript"
  ],
  "author": "Petar Stevovski",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pecko95/express-app-cli.git"
  },
  "bugs": {
    "url": "git+https://github.com/pecko95/express-app-cli/issues"
  },
  "homepage": "https://github.com/pecko95/express-app-cli#readme",
  "devDependencies": {
    "@types/node": "^14.14.2",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
